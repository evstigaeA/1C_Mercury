		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ОформленыеВСДЗаПериод.GUID_Меркурий КАК GUID_Меркурий,
	ОформленыеВСДЗаПериод.Предприятие КАК Предприятие,
	ОформленыеВСДЗаПериод.КонтрагентОтправитель КАК КонтрагентОтправитель,
	ОформленыеВСДЗаПериод.ПредприятиеОтправитель КАК ПредприятиеОтправитель,
	ОформленыеВСДЗаПериод.ТипВСД КАК ТипВСД,
	ОформленыеВСДЗаПериод.ДатаТТН КАК ДатаТТН,
	ОформленыеВСДЗаПериод.НомерТТН КАК НомерТТН,
	ОформленыеВСДЗаПериод.Продукция КАК Продукция,
	ОформленыеВСДЗаПериод.Артикул КАК Артикул,
	ОформленыеВСДЗаПериод.ДатаВыработки КАК ДатаВыработки,
	ОформленыеВСДЗаПериод.ДатаОкончанияСрокаГодности КАК ДатаОкончанияСрокаГодности,
	ОформленыеВСДЗаПериод.Количество КАК Количество,
	ОформленыеВСДЗаПериод.ПериодОформления КАК ПериодОформления,
	СтатусыВСДСрез.Период КАК ПериодПогашен,
	ОформленыеВСДЗаПериод.Целое КАК Целое,
	1 КАК Счетчик,
	ОформленыеВСДЗаПериод.Партия КАК Партия,
	СтатусыВСДСрез.СтатусВСД КАК СтатусВСД
ИЗ
	(ВЫБРАТЬ
		СтатусыВСДСрез.ВСД.ПредприятиеПолучатель КАК Предприятие,
		СтатусыВСДСрез.Период КАК ПериодОформления,
		СтатусыВСДСрез.ВСД КАК Документ,
		ВЫБОР
			КОГДА ВЫБОР
					КОГДА СтатусыВСДСрез.ВСД.Продукция.Вес = 0
						ТОГДА 0
					ИНАЧЕ СтатусыВСДСрез.ВСД.Количество / СтатусыВСДСрез.ВСД.Продукция.Вес - (ВЫРАЗИТЬ(СтатусыВСДСрез.ВСД.Количество / СтатусыВСДСрез.ВСД.Продукция.Вес КАК ЧИСЛО(10, 0)))
				КОНЕЦ = 0
				ТОГДА ИСТИНА
			ИНАЧЕ ЛОЖЬ
		КОНЕЦ КАК Целое,
		СтатусыВСДСрез.ВСД.GUID_Меркурий КАК GUID_Меркурий,
		СтатусыВСДСрез.ВСД.ПредприятиеОтправитель КАК ПредприятиеОтправитель,
		СтатусыВСДСрез.ВСД.КонтрагентОтправитель КАК КонтрагентОтправитель,
		СтатусыВСДСрез.ВСД.ТипВСД КАК ТипВСД,
		СтатусыВСДСрез.ВСД.НомерТТН КАК НомерТТН,
		СтатусыВСДСрез.ВСД.ДатаТТН КАК ДатаТТН,
		СтатусыВСДСрез.ВСД.Продукция КАК Продукция,
		СтатусыВСДСрез.ВСД.Продукция.Артикул КАК Артикул,
		СтатусыВСДСрез.ВСД.ДатаВыработкиНачало КАК ДатаВыработки,
		СтатусыВСДСрез.ВСД.ДатаОкончанияСрокаГодностиНачало КАК ДатаОкончанияСрокаГодности,
		СтатусыВСДСрез.ВСД.Количество КАК Количество,
		СтатусыВСДСрез.ВСД.НомерПартии КАК Партия
	ИЗ
		РегистрСведений.СтатусыВСД.СрезПоследних(
				,
				ВСД.ТипВСД = &amp;Возвратные
					И СтатусВСД = &amp;Оформлен
					И НЕ ВСД.ПометкаУдаления) КАК СтатусыВСДСрез
	ГДЕ
		СтатусыВСДСрез.Период МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода) КАК ОформленыеВСДЗаПериод
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.СтатусыВСД.СрезПоследних(
				,
				ВСД.ТипВСД = &amp;Возвратные
					И СтатусВСД &lt;&gt; &amp;Оформлен
					И НЕ ВСД.ПометкаУдаления) КАК СтатусыВСДСрез
		ПО ОформленыеВСДЗаПериод.Документ = СтатусыВСДСрез.ВСД</query>
	</dataSet>
