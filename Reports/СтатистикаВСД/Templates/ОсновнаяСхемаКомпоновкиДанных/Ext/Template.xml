		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	СтатусыВСДСрез.ВСД.ПредприятиеПолучатель КАК ВСДПредприятиеПолучатель,
	СтатусыВСДСрез.ВСД.ВидВСД КАК ВСДВидВСД,
	ВЫБОР
		КОГДА СтатусыВСДСрез.ВСД.ПредприятиеОтправитель = &amp;ПустоеПредприятие
			ТОГДА &amp;Импортный
		ИНАЧЕ ВЫБОР
				КОГДА СтатусыВСДСрез.ВСД.КонтрагентОтправитель.ХСДанон
					ТОГДА &amp;НаПеремещение
				ИНАЧЕ СтатусыВСДСрез.ВСД.ТипВСД
			КОНЕЦ
	КОНЕЦ КАК ВСДТипВСД,
	1 КАК Количество,
	НАЧАЛОПЕРИОДА(СтатусыВСДСрез.Период, ДЕНЬ) КАК ПериодСводной,
	СтатусыВСДСрез.ВСД КАК Документ,
	СтатусыВСДСрез.СтатусВСД КАК СтатусВСД,
	0 КАК Порядок,
	ВЫБОР
		КОГДА ВЫБОР
				КОГДА СтатусыВСДСрез.ВСД.Продукция.Вес = 0
					ТОГДА 0
				ИНАЧЕ СтатусыВСДСрез.ВСД.Количество / СтатусыВСДСрез.ВСД.Продукция.Вес - (ВЫРАЗИТЬ(СтатусыВСДСрез.ВСД.Количество / СтатусыВСДСрез.ВСД.Продукция.Вес КАК ЧИСЛО(10, 0)))
			КОНЕЦ = 0
			ТОГДА ИСТИНА
		ИНАЧЕ ЛОЖЬ
	КОНЕЦ КАК Целое,
	СтатусыВСДСрез.Период КАК Дата
ИЗ
	РегистрСведений.СтатусыВСД.СрезПоследних(&amp;ДатаРасчета, ) КАК СтатусыВСДСрез
ГДЕ
	СтатусыВСДСрез.Период МЕЖДУ &amp;ПериодНачала И &amp;ПериодОкончания
	И СтатусыВСДСрез.СтатусВСД = &amp;СтатусВСД
	И СтатусыВСДСрез.ВСД.ПредприятиеОтправитель &lt;&gt; &amp;ПустоеПредприятие
	И СтатусыВСДСрез.ВСД.ТипВСД &lt;&gt; &amp;ПроизводственныйВСД
	И НЕ СтатусыВСДСрез.ВСД.ПометкаУдаления

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	СтатусыВСД.ВСД.ПредприятиеПолучатель,
	СтатусыВСД.ВСД.ВидВСД,
	ВЫБОР
		КОГДА СтатусыВСД.ВСД.ПредприятиеОтправитель = &amp;ПустоеПредприятие
			ТОГДА &amp;Импортный
		ИНАЧЕ ВЫБОР
				КОГДА СтатусыВСД.ВСД.КонтрагентОтправитель.ХСДанон
					ТОГДА &amp;НаПеремещение
				ИНАЧЕ СтатусыВСД.ВСД.ТипВСД
			КОНЕЦ
	КОНЕЦ,
	1,
	КОНЕЦПЕРИОДА(ДОБАВИТЬКДАТЕ(&amp;ПериодНачала, ДЕНЬ, -1), ДЕНЬ),
	СтатусыВСД.ВСД,
	СтатусыВСД.СтатусВСД,
	1,
	ВЫБОР
		КОГДА ВЫБОР
				КОГДА СтатусыВСД.ВСД.Продукция.Вес = 0
					ТОГДА 0
				ИНАЧЕ СтатусыВСД.ВСД.Количество / СтатусыВСД.ВСД.Продукция.Вес - (ВЫРАЗИТЬ(СтатусыВСД.ВСД.Количество / СтатусыВСД.ВСД.Продукция.Вес КАК ЧИСЛО(10, 0)))
			КОНЕЦ = 0
			ТОГДА ИСТИНА
		ИНАЧЕ ЛОЖЬ
	КОНЕЦ,
	СтатусыВСД.Период
ИЗ
	РегистрСведений.СтатусыВСД.СрезПоследних(&amp;ДатаРасчета, ) КАК СтатусыВСД
ГДЕ
	СтатусыВСД.СтатусВСД = &amp;СтатусВСД
	И СтатусыВСД.ВСД.ПредприятиеОтправитель &lt;&gt; &amp;ПустоеПредприятие
	И СтатусыВСД.ВСД.ТипВСД &lt;&gt; &amp;ПроизводственныйВСД
	И СтатусыВСД.Период МЕЖДУ &amp;ДатаОтчета И КОНЕЦПЕРИОДА(ДОБАВИТЬКДАТЕ(&amp;ПериодНачала, ДЕНЬ, -1), ДЕНЬ)
	И НЕ СтатусыВСД.ВСД.ПометкаУдаления</query>
